{
  "name":"scum-and-villainy-es-es",
  "title":"Scum and Villainy - Español",
  "description":"Traducción para el sistema Scum and Villainy.",
  "version":"0.5.1",
  "authors": [
    {
      "name": "Gato Malefico"
    },
    {
      "name": "Gonzalo Ortiz"
    }
  ],

  "compatibility":{
    "minimum":11,
    "verified":"11"
  },

  "esmodules":[
    "babele-register.js"
  ],
   "languages":[
      {
         "lang": "es",
         "name": "Español",
         "path": "es.json"
      }
   ],

  "relationships":{
    "requires":[
      {
        "id":"babele",
        "type":"module",
        "manifest":"https://gitlab.com/riccisi/foundryvtt-babele/raw/master/module/module.json"
      }
    ],
    "systems":[
      {
        "id":"scum-and-villainy"
      }
    ]
  },

 "styles": [
    "./styles/style.css"
  ],

  "packs": [
   {
      "name": "ayudas-y-reglas",
      "label": "Ayudas y Reglas",
      "path": "packs/ayudas-y-reglas.db",
      "entity": "JournalEntry",
      "type": "JournalEntry",
      "module": "scum-and-villainy-es-es",
      "private": false
    },
    {
      "name": "macros-es",
      "label": "Macros ES",
      "path": "packs/macros-es.db",
      "entity": "Macro",
      "type": "Macro",
      "module": "scum-and-villainy-es-es",
      "private": false
    },
    {
      "name": "tablas-de-nivel-de-se-busca",
      "label": "Tablas de Nivel de Se Busca",
      "path": "packs/tablas-de-nivel-de-se-busca.db",
      "entity": "RollTable",
      "type": "RollTable",
      "module": "scum-and-villainy-es-es",
      "private": false
    },
    {
      "name": "tablas-de-trabajos",
      "label": "Tablas de Trabajos",
      "path": "packs/tablas-de-trabajos.db",
      "entity": "RollTable",
      "type": "RollTable",
      "module": "scum-and-villainy-es-es",
      "private": false
    }
  ],

  "url":"https://github.com/gortiz/scum-and-villainy-es-es",
  "manifest": "https://raw.githubusercontent.com/gortiz/scum-and-villainy-es-es/main/module.json",
  "download": "https://codeload.github.com/gortiz/scum-and-villainy-es-es/zip/main"
}
